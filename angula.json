{
    "image": "nicolasaigner/angular-docker:latest",
    "containerName": "frontend-angular",
    "volumes": [
        "${PWD}:/usr/src/app",
        "/usr/src/app/node_modules"
    ],
    "ports": "4200:4200"
    "environmentVariables": [
        {
            "name": "NODE_ENV",
            "value": "development"
        },
        {
            "name": "PORT",
            "value": "4200"
        }
    ]
}
